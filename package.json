    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "jshint": "^2.9.5",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.6",
    "npm": "^6.1.0",
